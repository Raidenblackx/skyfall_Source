<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity AvailabilityCompetition.
    -->
    <changeSet id="20191126021416-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="competition_id"
                                 baseTableName="availability_competition"
                                 constraintName="fk_availability_competition_competition_id"

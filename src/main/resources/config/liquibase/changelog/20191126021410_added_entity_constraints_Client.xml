<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Client.
    -->
    <changeSet id="20191126021410-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="document_type_id"
                                 baseTableName="client"
                                 constraintName="fk_client_document_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="client"
                                 constraintName="fk_client_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="system_user"/>

    </changeSet>
</databaseChangeLog>

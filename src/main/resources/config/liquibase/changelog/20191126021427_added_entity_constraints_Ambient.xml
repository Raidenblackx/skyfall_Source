        <addForeignKeyConstraint baseColumnNames="sede_id"
                                 baseTableName="ambient"
                                 constraintName="fk_ambient_sede_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sede"/>

    </changeSet>
</databaseChangeLog>

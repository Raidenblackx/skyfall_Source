<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity TimeStudy.
    -->
    <changeSet id="20191126021438-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="journey_instructor_id"
                                 baseTableName="time_study"
                                 constraintName="fk_time_study_journey_instructor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="journey_instructor"/>

        <addForeignKeyConstraint baseColumnNames="day_id"
                                 baseTableName="time_study"
                                 constraintName="fk_time_study_day_id"
                                 referencedColumnNames="id"
                                 referencedTableName="day"/>

    </changeSet>
</databaseChangeLog>

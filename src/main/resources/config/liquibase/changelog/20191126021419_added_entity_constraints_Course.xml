<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Course.
    -->
    <changeSet id="20191126021419-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="course_state_id"
                                 baseTableName="course"
                                 constraintName="fk_course_course_state_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course_state"/>

        <addForeignKeyConstraint baseColumnNames="working_day_id"
                                 baseTableName="course"
                                 constraintName="fk_course_working_day_id"
                                 referencedColumnNames="id"
                                 referencedTableName="working_day"/>

        <addForeignKeyConstraint baseColumnNames="program_id"
                                 baseTableName="course"
                                 constraintName="fk_course_program_id"
                                 referencedColumnNames="id"
                                 referencedTableName="program"/>

    </changeSet>
</databaseChangeLog>
